{
    "name": "lantongxue/yii3-swoole",
    "type": "library",
    "description": "Yii3 Swoole Extension",
    "version": "1.0.0",
    "keywords": [
        "yii3",
        "swoole",
        "async",
        "high-performance"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.1",
        "ext-swoole": "*",
        "nyholm/psr7": "^1.8",
        "yiisoft/yii-console": "^2.4",
        "yiisoft/yii-runner-http": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Yii3Swoole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii3Swoole\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/config": true
        }
    }
}